@model IEnumerable<nonleague.web.Entities.Month>
@{
    ViewData["Title"] = "Non League";
}

<div class="list-group">
@foreach(var month in Model)
{
  <a asp-controller="Fixture" asp-action="Index" asp-route-monthid=@month.ID class="list-group-item">
    <h4 class="list-group-item-heading">@month.Name</h4>
    <p class="list-group-item-text">@month.ID</p>
  </a>
}    
</div>